{
    "name": "siteAPI",
    "description": "Extension that extracts a website's content and posts it to an api",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    }
}