{
    "name": "siteAPI",
    "description": "Extension that extracts a website's content and posts it to an api",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs","scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
       "https://es.wikipedia.org/",
       "https://developer.mozilla.org/"
    ]
}